{
    "name": "ohdearapp/ohdear-cli",
    "description": "An Oh Dear CLI tool written in PHP with Laravel Zero.",
    "keywords": ["framework", "laravel", "console", "zero", "cli", "oh dear", "ohdear"],
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/nunomaduro/ohdear-cli/issues",
        "source": "https://github.com/nunomaduro/ohdear-cli"
    },
    "authors": [
        {
            "name": "Nuno Maduro",
            "email": "enunomaduro@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "illuminate/view": "^9.2",
        "laravel-zero/framework": "^9.1.1",
        "nunomaduro/termwind": "^1.6",
        "ohdearapp/ohdear-php-sdk": "^3.4.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "build": "php ohdear-cli app:build"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/ohdear-cli"]
}
